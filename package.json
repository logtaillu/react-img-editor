{
  "name": "react-img-editor",
  "version": "0.2.0",
  "description": "图片编辑器（微信截图编辑 PRO 版）",
  "config": {
    "port": 8001
  },
  "keywords": [
    "react-img-editor",
    "image-editor"
  ],
  "main": "lib/index",
  "module": "es/index",
  "scripts": {
    "start": "rc-tools run server",
    "build": "rc-tools run build",
    "gh-pages": "rc-tools run gh-pages",
    "pub": "rc-tools run update-self && rc-tools run pub",
    "lint": "eslint . --ext .js,jsx,.ts,.tsx",
    "prepublish": "rc-tools run compile",
    "compile": "rc-tools run compile"
  },
  "files": [
    "es",
    "lib",
    "assets/*.css"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/logtaillu/react-img-editor.git"
  },
  "author": "logtaillu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/logtaillu/react-img-editor/issues"
  },
  "homepage": "https://github.com/logtaillu/react-img-editor#readme",
  "devDependencies": {
    "@types/pubsub-js": "^1.5.18",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.4.0",
    "rc-tools": "^9.6.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "konva": "^4.1.2",
    "pubsub-js": "^1.8.0",
    "mdn-polyfills": "^5.20.0",
    "rc-tooltip": "^4.0.0-alpha.3",
    "react-draggable": "^4.2.0"
  }
}
